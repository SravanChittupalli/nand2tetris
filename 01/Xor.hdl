/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=abar);
    Not(in=b , out=bbar);
    And(a=a , b=bbar , out=out1);
    And(a=abar , b=b , out=out2);
    Or(a=out1 , b=out2 , out=out);
}
